{
    "name": "alex-kalanis/kw_images",
    "description": "Processing images in KWCMS",
    "keywords": [
        "images",
        "sql"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "alex-kalanis/kw_files": ">=2.0",
        "alex-kalanis/kw_mime": ">=1.0",
        "alex-kalanis/kw_paths": ">=1.0",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "php": ">=7.2.0"
    },
    "require-dev": {
        "alex-kalanis/kw_storage": ">=3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": ">=7.2"
    },
    "suggest": {
        "alex-kalanis/kw_storage": "Local storage for accessing resources"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_images\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
